entrypoint = "main.go"
run = ["go", "run", "."]
modules = ["go-1.21", "python-3.10"]

[nix]
channel = "stable-24_05"

[gitHubImport]
requiredFiles = [".replit"]

[deployment]
run = ["go", "run", "."]
ignorePorts = false
deploymentTarget = "gce"

[[ports]]
localPort = 3000
externalPort = 80
exposeLocalhost = true
